{
  "siteConfDir": "/etc/apache2/sites-available/",
  "siteDir": "/var/",
  "confDir": "/etc/sitegen/",
  "certRenewTime": 5356800,
  "letsencryptCommand": "letsencrypt",
  "letsencryptArgs": [
    "--agree-tos",
    "--renew-by-default",
    "--webroot",
    "--webroot-path",
    "/tmp/acme-challenge/",
    "--server",
    "https://acme-v01.api.letsencrypt.org/directory",
    "certonly"
  ],
  "letsencryptDir": "/etc/letsencrypt/live/",
  "certDir": "/etc/ssl/private/"
}